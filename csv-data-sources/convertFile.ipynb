    "import json\n",
    "file = input('Insert Filename')\n",
    "\n",
    "with open('./data/'+file, 'r') as f:\n",
    "  LIST = f.read().split('\\n')\n",
    "f.close()\n",
    "\n",
    "nameFile = file.split('.')[0]\n",
    "\n",
    "f = open('./data/'+nameFile+'.json', 'w')\n",
    "f.write(json.dumps(LIST))\n",
    "f.close()"
   ]
  }
